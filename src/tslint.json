{
  "extends": "../tslint.json",
  "rules": {
    "no-shadowed-variable": false,
    "curly": false,
    "no-bitwise": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
